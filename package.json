{
  "name": "fuar2026-rollup-pdf",
  "private": true,
  "version": "1.0.0",
  "description": "Convert turkce.html and ingilizce.html to print-ready PDFs using Puppeteer.",
  "type": "commonjs",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "pdf": "node pdf.js",
    "pdf:kartvizit": "node pdf-kartvizit.js",
    "pdf:kartvizit:raster": "node pdf-kartvizit.js --raster",
    "pdf:brochure": "node pdf-brochure.js",
    "pdf:brochure:raster": "node pdf-brochure.js --raster"
  },
  "dependencies": {
    "puppeteer": "24.34.0"
  }
}


